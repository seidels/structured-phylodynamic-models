    <parameter estimate="false" name="popSizesScaleFactor">1.0</parameter>
    <typeSet id="typeSet" spec="TypeSet" 
	     typeTraitSet="@typeTraitSet"/> 
  </migrationModel>

<!-- calculate forward in time migration rate -->
<parameter spec="feast.expressions.ExpCalculator"
	       id="m_01"
               value=" rateMatrix_[1] * popSizes_[1] /popSizes_[0]"
               > <!-- sr/ (sr + dr ) -->
  <arg idref="popSizes_"/>
  <arg idref="rateMatrix_" />
</parameter>

<parameter spec="feast.expressions.ExpCalculator"
	       id="m_10"
               value=" rateMatrix_[0] * popSizes_[0] /popSizes_[1]"
               > <!-- sr/ (sr + dr ) -->
  <arg idref="popSizes_"/>
  <arg idref="rateMatrix_" />
</parameter>



  <map name="Uniform" >beast.math.distributions.Uniform</map>
  <map name="Exponential" >beast.math.distributions.Exponential</map>
  <map name="LogNormal" >beast.math.distributions.LogNormalDistributionModel</map>
  <map name="Normal" >beast.math.distributions.Normal</map>
  <map name="Beta" >beast.math.distributions.Beta</map>
  <map name="Gamma" >beast.math.distributions.Gamma</map>
  <map name="LaplaceDistribution" >beast.math.distributions.LaplaceDistribution</map>
  <map name="prior" >beast.math.distributions.Prior</map>
  <map name="InverseGamma" >beast.math.distributions.InverseGamma</map>
  <map name="OneOnX" >beast.math.distributions.OneOnX</map>


  <run id="mcmc" spec="MCMC" chainLength="10000000000">
    <state id="state" storeEvery="5000">
      <!-- load starting tree from tree simulation file and do fixed
	   inference, e.g. change only the colors and time points when colors
	   change on the tree -->
      <stateNode id="inputTree"
		 spec="beast.evolution.tree.MultiTypeTreeFromUntypedNewick"
		 adjustTipHeights="false"

	>
	<migrationModel idref="migModel" />
	<typeTrait idref="typeTraitSet" />
	<typeSet idref="typeSet" />
	
	<trait idref="timeTraitSet"/>
	<taxonset idref="taxa1"/>
      </stateNode>
      
      <!-- specify boundaries on parameters -->
      <parameter id="clockRate_"
		 name="stateNode">1.0</parameter> 
      <parameter id="popSizes_" dimension="2"
		 name="stateNode" upper="5000" lower="0.001">1.0
      1.0</parameter> 
      <parameter id="rateMatrix_" dimension="2"
		 name="stateNode" upper="100" lower="0">0.5
      1.0</parameter> 
    </state>
    
    <!-- specify distribution -->
    <distribution id="posterior" spec="util.CompoundDistribution">
      <distribution id="prior" spec="util.CompoundDistribution">
	<prior id="ClockPrior" name="distribution" x="@clockRate_">
	  <Uniform id="Uniform.0" name="distr" upper="Infinity"/>
	</prior>
	<!-- prior on population size -->
	<prior id="popSizesPrior" name="distribution" x="@popSizes_">
	  <OneOnX name="distr"/> 
	</prior> 
	
 	<!-- <prior id="m_01_prior" name="distribution" x="@m_01">
	   <Exponential name="distr" mean="1.0"/>
	 </prior>
	 <prior id="m_10_prior" name="distribution" x="@m_10">
	   <Exponential name="distr" mean="1.0"/>
	 </prior> -->

	<prior id="rateMatrixPrior" name="distribution" x="@rateMatrix_">
	<Exponential name="distr" mean="1.0"/>
	</prior> 


	<distribution id="structuredCoalescent.t:h3n2_2deme" spec="multitypetree.distributions.StructuredCoalescentTreeDensity" multiTypeTree="@inputTree">
	  <migrationModel id="migModel_" spec="beast.evolution.tree.SCMigrationModel" popSizes="@popSizes_" rateMatrix="@rateMatrix_" typeSet="@typeSet">
            <parameter id="rateMatrixScaleFactor.t:h3n2_2deme" estimate="false" name="rateMatrixScaleFactor">1.0</parameter>
            <parameter id="popSizesScaleFactor.t:h3n2_2deme" estimate="false" name="popSizesScaleFactor">1.0</parameter>
	  </migrationModel>
	</distribution>
      </distribution>
      <distribution id="likelihood" spec="util.CompoundDistribution">
      </distribution>
    </distribution>

    <!-- operators; 
    operators on the tree are set to 0 for fixed tree inference--> 
    <operator id="STX.t:h3n2_2deme" spec="multitypetree.operators.TypedSubtreeExchange" migrationModel="@migModel_" multiTypeTree="@inputTree" weight="0.0"/>

    <operator id="TWB.t:h3n2_2deme" spec="multitypetree.operators.TypedWilsonBalding" alpha="0.2" migrationModel="@migModel_" multiTypeTree="@inputTree" weight="0.0"/>

    <operator id="NR.t:h3n2_2deme" spec="multitypetree.operators.NodeRetype" migrationModel="@migModel_" multiTypeTree="@inputTree" weight="2.0"/>

    <operator id="NSR1.t:h3n2_2deme" spec="multitypetree.operators.NodeShiftRetype" migrationModel="@migModel_" multiTypeTree="@inputTree" rootOnly="true" weight="0.0"/>

    <operator id="NSR2.t:h3n2_2deme" spec="multitypetree.operators.NodeShiftRetype" migrationModel="@migModel_" multiTypeTree="@inputTree" noRoot="true" weight="0.0"/>

    <operator id="MTU.t:h3n2_2deme" spec="multitypetree.operators.MultiTypeUniform" includeRoot="true" migrationModel="@migModel_" multiTypeTree="@inputTree" weight="0.0"/>

    <operator id="MTTS.t:h3n2_2deme" spec="multitypetree.operators.MultiTypeTreeScale" migrationModel="@migModel_" multiTypeTree="@inputTree" scaleFactor="0.98" useOldTreeScaler="true" weight="0.0"/>

    <operator id="popSizesScaler.t:h3n2_2deme" spec="ScaleOperator" parameter="@popSizes_" scaleFactor="0.8" weight="1.0"/>

    <operator id="rateMatrixScaler.t:h3n2_2deme" spec="ScaleOperator"
	      parameter="@rateMatrix_" scaleFactor="0.8"
	      weight="1.0"/>
	      
	      <operator id="clockRateScaler.t:h3n2_2deme" spec="ScaleOperator"
	      parameter="@clockRate_" scaleFactor="0.8"
	      weight="1.0"/>

    <operator spec="UpDownOperator" scaleFactor="0.8" weight="1.0"
          elementWise="true" differentRandomIndex="true">
                <up idref="popSizes_"/>
                <down idref="popSizes_"/>
    </operator>

    <operator spec="UpDownOperator" scaleFactor="0.8" weight="1.0"
          elementWise="true" differentRandomIndex="true">
                <up idref="popSizes_"/>
                <down idref="rateMatrix_"/>
    </operator>

    <operator spec="UpDownOperator" scaleFactor="0.8" weight="1.0"
          elementWise="true" differentRandomIndex="true">
                <up idref="rateMatrix_"/>
                <down idref="rateMatrix_"/>
    </operator>

<!-- loggers -->
    <logger id="tracelog" fileName="$(filebase).$(seed).log" logEvery="10000">
      <log idref="posterior"/>
      <log idref="likelihood"/>
      <log idref="prior"/>
      <log id="typeLengths.t:h3n2_2deme" spec="multitypetree.util.TypeLengths" migrationModel="@migModel_" multiTypeTree="@inputTree"/>
      <log id="changeCounts.t:h3n2_2deme" spec="multitypetree.util.TypeChangeCounts" migrationModel="@migModel_" multiTypeTree="@inputTree"/>
      <log id="nodeTypeCounts.t:h3n2_2deme" spec="multitypetree.util.NodeTypeCounts" migrationModel="@migModel_" multiTypeTree="@inputTree"/>
      <log id="rootTypeLogger.t:h3n2_2deme" spec="multitypetree.util.TreeRootTypeLogger" multiTypeTree="@inputTree"/>
      <log id="migModel_Logger.t:h3n2_2deme"
	   spec="multitypetree.util.MigrationModelLogger"
	   migrationModel="@migModel_" multiTypeTree="@inputTree"/>
      <log idref="m_01" />
      <log idref="m_10" />
      <log idref="clockRate_"/>
    </logger>

    <logger id="screenlog" logEvery="10000">
      <log idref="posterior"/>
      <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
      <log idref="likelihood"/>
      <log idref="prior"/>
    </logger>

